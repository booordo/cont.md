+b.header
	+e.panel.inner
		+e.logo
			include ../../components/logo/_logo
		+e.contact
			include ../../components/contact/_contact
		+e.social
			include ../../components/social/_social
	+e.navbar
		+b.inner
			+b.UL.navbar_top
				each item in menu
					+e.LI.item_top
						+e.A.link_top(href= item.link)= item.title
